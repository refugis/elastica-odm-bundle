<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults>
            <tag name="elastica.type" />
        </defaults>
        
        <service id="refugis.odm.elastica.type.binary" class="Refugis\ODM\Elastica\Type\BinaryType" />
        <service id="refugis.odm.elastica.type.boolean" class="Refugis\ODM\Elastica\Type\BooleanType" />
        <service id="refugis.odm.elastica.type.completion" class="Refugis\ODM\Elastica\Type\CompletionType" />
        <service id="refugis.odm.elastica.type.date_time_immutable" class="Refugis\ODM\Elastica\Type\DateTimeImmutableType" />
        <service id="refugis.odm.elastica.type.date_time" class="Refugis\ODM\Elastica\Type\DateTimeType" />
        <service id="refugis.odm.elastica.type.float" class="Refugis\ODM\Elastica\Type\FloatType" />
        <service id="refugis.odm.elastica.type.geo_point" class="Refugis\ODM\Elastica\Type\GeoPointType" />
        <service id="refugis.odm.elastica.type.geo_shape" class="Refugis\ODM\Elastica\Type\GeoShapeType" />
        <service id="refugis.odm.elastica.type.integer" class="Refugis\ODM\Elastica\Type\IntegerType" />
        <service id="refugis.odm.elastica.type.ip" class="Refugis\ODM\Elastica\Type\IpType" />
        <service id="refugis.odm.elastica.type.percolator" class="Refugis\ODM\Elastica\Type\PercolatorType" />
        <service id="refugis.odm.elastica.type.raw" class="Refugis\ODM\Elastica\Type\RawType" />
        <service id="refugis.odm.elastica.type.string" class="Refugis\ODM\Elastica\Type\StringType" />
    </services>
</container>
